{
    "name": "freeflarum/flarum-anonfiles-upload",
    "description": "Modified Imgur Upload extension by Matteo to be able to upload files to Anonfiles.com.",
    "type": "flarum-extension",
    "license": "GPL-3.0",
    "authors": [
      {
        "name": "Matteo Contrini",
        "email": "matteo@contrini.it"
      },
      {
        "name": "Matteo Pompili",
        "email": "matpompili@gmail.com"
      },
      {
        "name": "FreeFlarum",
        "email": "info@freeflarum.com"
      }
    ],
    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
    "extra": {
        "flarum-extension": {
            "title": "Anonfiles Upload",
            "icon": {
                "name": "fas fa-clipboard",
                "backgroundColor": "#272727",
                "color": "#85BF25"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "AnonfilesUpload\\": "src/"
        }
    }
}
